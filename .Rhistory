print_guess_statistic = function(candidate, count){
if (count == 0) {
c(number, verb, modifier) %<-% c("no", "are", "s")
} else if (count == 1) {
c(number, verb, modifier) %<-% c("1", "is", "")
} else {
c(number, verb, modifier) %<-% c(toString(count), "are", "s")
}
guess_message = sprintf("There %s %s %s%s", parts, number, candidate, modifier)
print(guess_message)
}
print_guess_statistic("hello", 2)
